{
	"plugins": [ "plugins/markdown" ],
	"source": {
		"include": "src"
	},
	"opts": {
		"package": "package.json",
		"readme": "doc/home.md",
		"tutorials": "doc/tutorials",
		"template": "node_modules/tui-jsdoc-template"
	},
	"templates": {
		"default": {
			"useLongnameInNav": true,
			"staticFiles": {
				"include": [ "doc/static" ]
			}
		},
		"css": [ "styles/tui-doc-for-kokopu.css" ],
		"name": "Kokopu documentation",
		"footerText": "Kokopu documentation"
	}
}
